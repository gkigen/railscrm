<% @page_title = "Show Account" %>

<%= link_to("<< Back to List", {:action => 'index'} ) %>

<div >
  <h2>Show account</h2>

  <table class="table table-striped table-bordered table-condensed" summary="Account detail view">
    <tr>
      <th>company</th>
      <td><%= @account.company %></td>
    </tr>
    <tr>
      <th>address</th>
      <td><%= @account.address %></td>
    </tr>
    <tr>
      <th>mobile</th>
      <td><%= @account.mobile %></td>
    </tr>
    <tr>
      <th>industry</th>
      <td><%= @account.industry %></td>
    </tr>
    <tr>
      <th>county</th>
      <td><%= @account.county %></td>
    </tr>
  </table>

</div>
