<% @page_title = "Show Lead" %>

<%= link_to("<< Back to List", {:action => 'index'} ) %>

<div >
  <h2>Show Lead</h2>

  <table  class="table table-striped table-bordered table-condensed" summary="Lead detail view">
    <tr>
      <th>company</th>
      <td><%= @lead.company %></td>
    </tr>
    <tr>
      <th>address</th>
      <td><%= @lead.address %></td>
    </tr>
    <tr>
      <th>email</th>
      <td><%= @lead.email %></td>
    </tr>
    <tr>
      <th>mobile</th>
      <td><%= @lead.mobile %></td>
    </tr>
    <tr>
      <th>industry</th>
      <td><%= @lead.industry %></td>
    </tr>
    <tr>
      <th>leadsource</th>
      <td><%= @lead.leadsource %></td>
    </tr>
    <tr>
      <th>county</th>
      <td><%= @lead.county %></td>
    </tr>
    <tr>
      <th>converted</th>
      <td><%= @lead.converted %></td>
    </tr>
  </table>

</div>
